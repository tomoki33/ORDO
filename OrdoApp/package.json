{
  "name": "ordo-app",
  "displayName": "Ordo - AI Home Management",
  "version": "0.1.0",
  "description": "AI-powered home inventory management app with smart product recognition",
  "author": "tomoki33",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint:check": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "test": "jest",
    "test:unit": "jest --testPathPattern=__tests__/unit --coverage",
    "test:integration": "jest --testPathPattern=__tests__/integration",
    "test:e2e": "detox test",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false --passWithNoTests",
    "test:coverage": "jest --coverage --coverageReporters=text-lcov --coverageReporters=html",
    "type-check": "tsc --noEmit",
    "clean": "npx react-native clean",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:android:debug": "cd android && ./gradlew assembleDebug",
    "build:ios": "npx react-native build-ios --mode Release",
    "build:ios:debug": "npx react-native build-ios --mode Debug",
    "analyze": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output bundle.js --analyze",
    "postinstall": "cd ios && pod install && cd ..",
    "demo": "node scripts/run-demo.js",
    "demo:phase10": "node -e \"require('./src/utils/Phase10ExtensionDemo').phase10ExtensionDemo.runCompleteDemo()\"",
    "demo:phase11": "node -e \"require('./src/utils/Phase11NewFeaturesDemo').phase11NewFeaturesDemo.runCompleteDemo()\""
  },
  "keywords": [
    "react-native",
    "mobile-app",
    "ai",
    "home-management",
    "inventory",
    "product-recognition"
  ],
  "dependencies": {
    "@bam.tech/react-native-image-resizer": "^3.0.11",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-ml-kit/barcode-scanning": "^2.0.0",
    "@react-native/new-app-screen": "0.81.0",
    "@react-navigation/bottom-tabs": "^7.4.6",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.7",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-backend-cpu": "^4.22.0",
    "@tensorflow/tfjs-backend-webgl": "^4.22.0",
    "@tensorflow/tfjs-react-native": "^1.0.0",
    "@types/react-native-sqlite-storage": "^6.0.5",
    "openai": "^5.19.1",
    "react": "19.1.0",
    "react-native": "0.81.0",
    "react-native-device-info": "^14.0.4",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-image-to-base64": "^0.1.0",
    "react-native-paper": "^5.14.5",
    "react-native-permissions": "^5.4.2",
    "react-native-push-notification": "^8.1.1",
    "react-native-reanimated": "^4.1.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.15.4",
    "react-native-sqlite-storage": "^6.0.1",
    "react-native-svg": "^15.12.1",
    "react-native-vector-icons": "^10.3.0",
    "react-native-vision-camera": "^4.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.4",
    "@react-native/eslint-config": "0.75.4",
    "@react-native/metro-config": "0.75.4",
    "@react-native/typescript-config": "0.75.4",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.3.1",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
